@model IEnumerable<Vente.Models.Proprietaire>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomEntreprise)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdresseEntreprise)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Utilisateur)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomEntreprise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdresseEntreprise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Utilisateur.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDUtilisateur">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDUtilisateur">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDUtilisateur">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
